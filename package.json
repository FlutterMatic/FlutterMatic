{
  "name": "flutter-matic",
  "displayName": "Flutter Matic",
  "description": "Easiest way to get flutter up and running",
  "version": "1.0.4",
  "publisher": "FlutterMatic",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "repository": "https://github.com/FlutterMatic/FlutterMatic",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "./media/logo.png",
          "id": "fluttermatic-sidebar",
          "title": "FlutterMatic"
        }
      ]
    },
    "views": {
      "fluttermatic-sidebar": [
        {
          "type": "webview",
          "id": "fluttermatic.side",
          "name": "Quick Access"
        }
      ]
    },
    "commands": [
      {
        "command": "flutter-matic.dashboard",
        "title": "Dashboard"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "format": "prettier src/**/*.ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "prettier": "^2.2.1",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {}
}
